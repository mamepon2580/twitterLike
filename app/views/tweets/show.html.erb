<%= render 'head', tweet: @tweet %>
<p id="notice"><%= notice %></p>

<p>
  <strong>User:</strong>
  <%= @tweet.user_id %>
</p>

<p>
  <strong>User name:</strong>
  <%= @tweet.user_name %>
</p>

<p>
  <strong>Tweet time:</strong>
  <%= @tweet.tweet_time %>
</p>

<p>
  <strong>Content:</strong>
  <%= @tweet.content %>
</p>

<%= link_to 'Edit', edit_tweet_path(@tweet) %> |
<%= link_to 'Back', tweets_path %>
